{
    "private": true,
    "sideEffects": false,
    "type": "module",
    "scripts": {
        "dev": "remix vite:dev",
        "db:bootstrap": "rm -rf ./app/data/local.db; touch ./app/data/local.db; drizzle-kit push:sqlite",
        "format": "prettier -w .",
        "build": "remix vite:build",
        "start": "remix-serve ./build/server/index.js"
    },
    "dependencies": {
        "@headlessui/react": "^1.7.18",
        "@heroicons/react": "^2.1.3",
        "@libsql/client": "^0.6.0",
        "@remix-run/node": "^2.8.1",
        "@remix-run/react": "^2.8.1",
        "@remix-run/serve": "^2.8.1",
        "@unpic/react": "^0.1.11",
        "drizzle-orm": "^0.30.7",
        "isbot": "^4.1.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@remix-run/dev": "^2.8.1",
        "@types/react": "^18.2.20",
        "@types/react-dom": "^18.2.7",
        "autoprefixer": "^10.4.19",
        "drizzle-kit": "^0.20.14",
        "postcss": "^8.4.38",
        "prettier": "^3.2.5",
        "prettier-plugin-tailwindcss": "^0.5.13",
        "tailwindcss": "^3.4.3",
        "typescript": "^5.1.6",
        "vite": "^5.1.0",
        "vite-tsconfig-paths": "^4.2.1"
    },
    "prettier": {
        "printWidth": 100,
        "tabWidth": 4,
        "arrowParens": "avoid",
        "singleQuote": false,
        "semi": false,
        "plugins": [
            "prettier-plugin-tailwindcss"
        ]
    },
    "engines": {
        "node": ">=18.0.0"
    }
}
